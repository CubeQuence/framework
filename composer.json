{
    "name": "cubequence/framework",
    "description": "The CubeQuence Framework",
    "keywords": [
        "framework",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=8.0",
        "cubequence/crypto": "^1.1",
        "cubequence/db": "^1.0",
        "cubequence/file": "^1.0",
        "cubequence/helpers": "^1.0",
        "cubequence/oauth": "^1.1",
        "cubequence/ratelimit": "^1.0",
        "cubequence/request": "^1.0",
        "miladrahimi/phprouter": "^5.1",
        "mnapoli/silly": "^1.7",
        "respect/validation": "^2.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "nunomaduro/phpinsights": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\": "src/CubeQuence/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CubeQuence/CubeQuence/issues",
        "source": "https://github.com/CubeQuence/CubeQuence"
    }
}
