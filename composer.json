{
    "name": "cubequence/framework",
    "description": "The CubeQuence Framework",
    "keywords": [
        "framework",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "catfan/medoo": "^1.7",
        "firebase/php-jwt": "^5.2",
        "guzzlehttp/guzzle": "^6.5",
        "miladrahimi/phprouter": "^4.0",
        "phpseclib/phpseclib": "^2.0",
        "respect/validation": "^2.0",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\": "src/CubeQuence/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/Luca-Castelnuovo/CubeQuence/issues",
        "source": "https://github.com/Luca-Castelnuovo/CubeQuence"
    }
}
