{
    "name": "cubequence/framework",
    "description": "The CubeQuence Framework",
    "keywords": [
        "framework",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=8.0",
        "catfan/medoo": "^1.7",
        "fzaninotto/faker": "^1.9",
        "guzzlehttp/guzzle": "^7.0",
        "miladrahimi/phprouter": "^5.0",
        "mnapoli/silly": "^1.7",
        "paragonie/halite": "^4",
        "ramsey/uuid": "^4.0",
        "respect/validation": "^2.0",
        "robmorgan/phinx": "^0.12.1",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\": "src/CubeQuence/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CubeQuence/CubeQuence/issues",
        "source": "https://github.com/CubeQuence/CubeQuence"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "dev-master"
    }
}
